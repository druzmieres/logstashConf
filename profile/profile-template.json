{
  "template": "profile*",
  "version": 1,
  "settings": {
    "max_result_window": 15000
  },
  "mappings": {
    "*": {
      "properties": {
        "route": {
          "type": "keyword"
        },
        "licensePlate": { 
          "type": "keyword"
        },
        "authStopCode" : { 
          "type": "keyword"
        },
        "userStopCode": {
          "type": "keyword"
        },
        "userStopName": { 
          "type": "text"
        },
        "expeditionStartTime": { 
          "type": "date"
        },
        "expeditionEndTime": { 
          "type": "date"
        },
        "fulfillment" : { 
          "type": "keyword"
        },
        "expeditionStopOrder": {
          "type": "integer"
        },
        "expeditionDayId" : { 
          "type": "integer"
        },
        "stopDistanceFromPathStart" : { 
          "type": "half_float"
        },
        "expandedBoarding" : { 
          "type": "half_float"
        },
        "expandedAlighting": { 
          "type": "half_float"
        },
        "loadProfile": { 
          "type": "half_float"
        },
        "busCapacity" : { 
          "type": "integer"
        },
        "expeditionStopTime" : { 
          "type": "date",
          "null_value": "0"
        },
        "timePeriodInStartTime": {
          "type": "keyword"
        },
        "timePeriodInStopTime": {
          "type": "keyword"
        },
        "dayType": {
          "type": "keyword"
        },
        "busStation": {
          "type": "boolean"
        }
      }
    }
  }
}
