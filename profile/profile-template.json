{
  "template": "profile*",
  "version": 1,
  "settings": {
    "max_result_window": 15000,
    "index": {
      "analysis" : {
        "analyzer" : {
          "autocomplete_analyzer" : {
            "type" : "custom",
            "tokenizer" : "lowercase",
            "filter"    : ["asciifolding", "title_ngram"]
          }
        },
        "filter" : {
          "title_ngram" : {
            "type" : "nGram",
            "min_gram" : 3,
            "max_gram" : 5
          }
        }
      }
    }
  },
  "mappings": {
    "*": {
      "properties": {
        "route": {
          "type": "keyword"
        },
        "licensePlate": { 
          "type": "keyword"
        },
        "authStopCode" : { 
          "type": "text",
          "analyzer": "autocomplete_analyzer"
        },
        "userStopCode": {
          "type": "text",
          "analyzer": "autocomplete_analyzer"
        },
        "userStopName": { 
          "type": "text",
          "analyzer": "autocomplete_analyzer"
        },
        "expeditionStartTime": { 
          "type": "date"
        },
        "expeditionEndTime": { 
          "type": "date"
        },
        "fulfillment" : { 
          "type": "keyword"
        },
        "expeditionStopOrder": {
          "type": "integer"
        },
        "expeditionDayId" : { 
          "type": "integer"
        },
        "stopDistanceFromPathStart" : { 
          "type": "half_float"
        },
        "expandedBoarding" : { 
          "type": "half_float"
        },
        "expandedAlighting": { 
          "type": "half_float"
        },
        "loadProfile": { 
          "type": "half_float"
        },
        "busCapacity" : { 
          "type": "integer"
        },
        "expeditionStopTime" : { 
          "type": "date",
          "null_value": "0"
        },
        "timePeriodInStartTime": {
          "type": "keyword"
        },
        "timePeriodInStopTime": {
          "type": "keyword"
        },
        "dayType": {
          "type": "keyword"
        },
        "busStation": {
          "type": "boolean"
        }
      }
    }
  }
}
